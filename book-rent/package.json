{
	"name": "out",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.development -- next dev --port 4000",
		"devP": "dotenv -e .env.production -- next dev --port 4000",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"db:pull": "dotenv -e .env.development -- prisma db pull",
		"migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
		"db:push": "dotenv -e .env.development -- prisma db push",
		"migrate:deploy": "dotenv -e .env.production -- prisma migrate reset --force",
		"generate": "dotenv -e .env.development -- prisma generate",
		"db:seed": "dotenv -e .env.development -- tsx  seed.ts",
		"db:migrate:deploy": "dotenv -e .env.production -- prisma migrate deploy",
		"db:seed:prod": "dotenv -e .env.production -- tsx seed.ts"
	},
	"dependencies": {
		"@casl/ability": "^6.7.1",
		"@casl/prisma": "^1.4.1",
		"@casl/react": "^4.0.0",
		"@emotion/react": "^11.13.0",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^5.16.6",
		"@mui/material": "^5.16.6",
		"@mui/x-charts": "^7.12.0",
		"@mui/x-date-pickers": "^7.12.0",
		"@prisma/client": "5.18.0",
		"@t3-oss/env-nextjs": "^0.11.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@vercel/blob": "^0.23.4",
		"jose": "^5.6.3",
		"jsonwebtoken": "^9.0.2",
		"material-react-table": "^2.13.1",
		"next": "14.2.5",
		"nextjs-toploader": "^1.6.12",
		"react": "^18",
		"react-dom": "^18",
		"react-hot-toast": "^2.4.1",
		"recharts": "^2.12.7",
		"sharp": "^0.33.4",
		"tailwindcss": "^3.4.9",
		"use-debounce": "^10.0.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"prisma": "^5.18.0",
		"typescript": "^5"
	}
}
